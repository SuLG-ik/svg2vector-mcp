{
  "name": "svg2vector-mcp",
  "version": "1.1.0",
  "description": "MCP server for converting SVG to Android Vector Drawable XML format",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "svg2vector-mcp": "./src/index.js"
  },
  "exports": {
    ".": "./src/index.js",
    "./converter": "./src/converter/index.js",
    "./parser": "./src/parser/index.js",
    "./tree": "./src/tree/index.js",
    "./utils": "./src/utils/index.js"
  },
  "files": [
    "src",
    "README.md"
  ],
  "scripts": {
    "start": "node src/index.js",
    "test": "node --test test/*.test.js",
    "test:watch": "node --test --watch test/*.test.js",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "mcp",
    "svg",
    "vector-drawable",
    "android",
    "converter",
    "model-context-protocol"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuLG-ik/svg2vector-mcp.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@xmldom/xmldom": "^0.8.10",
    "axios": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
